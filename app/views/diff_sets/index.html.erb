<p id="notice"><%= notice %></p>

<h1>Diff Sets</h1>

<div class="row">
  <div class="col-sm-10">
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Diffs</th>
          <th>Actions</th>
        </tr>
      </thead>

      <tbody>
        <% @diff_sets.each do |diff_set| %>
          <tr>
            <td><%= diff_set.diff_ids.join(',') %></td>
            <td>
              <%= link_to 'Show', diff_set %>
              <%= link_to 'Edit', edit_diff_set_path(diff_set) %>
              <%= link_to 'Destroy', diff_set, method: :delete, data: { confirm: 'Are you sure?' } %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

<br>
