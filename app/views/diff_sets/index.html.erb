<p id="notice"><%= notice %></p>

<h1>Diff Sets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diff_sets.each do |diff_set| %>
      <tr>
        <td><%= diff_set.diff_ids.join(',') %></td>
        <td><%= link_to 'Show', diff_set %></td>
        <td><%= link_to 'Edit', edit_diff_set_path(diff_set) %></td>
        <td><%= link_to 'Destroy', diff_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diff Set', new_diff_set_path %>
