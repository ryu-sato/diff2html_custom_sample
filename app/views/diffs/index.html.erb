<p id="notice"><%= notice %></p>

<h1>Diffs</h1>

<div class="row">
  <div class="col-sm-10">
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>ID</th>
          <th>Actions</th>
        </tr>
      </thead>

      <tbody>
        <% @diffs.each do |diff| %>
          <tr>
            <td><%= diff.id %></td>
            <td>
              <%= link_to 'Show', diff %>
              <%= link_to 'Edit', edit_diff_path(diff) %>
              <%= link_to 'Destroy', diff, method: :delete, data: { confirm: 'Are you sure?' } %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

<br>

<%= link_to 'New Diff', new_diff_path %>
